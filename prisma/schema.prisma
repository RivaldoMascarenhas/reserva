generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  company    String?
  password   String
  agree      Boolean
  created_at DateTime @default(now())

  @@map("users")
}
